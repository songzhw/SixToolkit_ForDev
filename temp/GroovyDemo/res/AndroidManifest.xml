<?xml version="1.0" encoding="UTF-8"?>
<manifest package="ca.six.demo.advanced2021">
    <uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
                     android:name="android.permission.INTERNET"/>
    <uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
                     android:name="android.permission.CAMERA"/>
    <uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
                     android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
                     android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission xmlns:android="http://schemas.android.com/apk/res/android"
                     android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application xmlns:android="http://schemas.android.com/apk/res/android"
                 android:supportsRtl="false" android:label="@string/app_name"
                 android:theme="@style/Theme.Advanced2021" android:icon="@mipmap/ic_launcher"
                 android:allowBackup="false" android:roundIcon="@mipmap/ic_launcher_round">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".core.security.rsa.RsaDecryptDemo"/>
        <activity android:name=".core.security.AesDemo"/>
        <activity android:name=".hi.FirstActivity" android:screenOrientation="portrait"
                  android:configChanges="keyboardHidden|orientation"/>
    </application>
</manifest>
